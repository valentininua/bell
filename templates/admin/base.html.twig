    <script type="text/javascript" src="/js/currency.js"></script>
    <script>
        var App = {
            Invest : function (id) {
                var idValue = document.getElementById( 'value' + id + 'Modal'  ).value;

                var currencyBalance = currency(App.balance).subtract(idValue)
                if( currencyBalance >=0) {

                    document.getElementById('balance-'+ id ).innerHTML = currency( document.getElementById('value'+ id +'Modal').value )
                        .add( document.getElementById('balance-'+ id ).innerHTML ).format();

                    if( currency(idValue).intValue < 10000  ){
                        alert(' Минимальная сумма инвестиций  100 USD');
                        return;
                    }

                    $.post( "/admin/ajax/invest", { idValue: idValue, id: id }, function( e ) {
                        document.getElementById('balance').innerHTML = currency(currencyBalance, { separator: ' ' }).format();
                        document.getElementById('available_funds').innerHTML = currency(currencyBalance, { separator: ' ' }).format();
                        App.balance = currencyBalance;
                        $('#' + id + 'Modal').modal('hide');
                    });
                } else {
                    alert('Недостаточно денег на счете')
                }
            },
            balance: currency({% if report.available_funds is defined %}{{ report.available_funds }}{% else %}0{% endif %}).format(),
        };
    </script>

